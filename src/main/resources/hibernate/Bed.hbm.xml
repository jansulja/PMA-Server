<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tim11.pma.ftn.pmaprojekat.model" auto-import="true">
   <class name="Bed" table="BED" mutable="true" dynamic-update="false" dynamic-insert="false" select-before-update="false" lazy="true" abstract="false">
      <id name="bedId">
         <column name="BED_ID" sql-type="int" not-null="true"/>
      </id>
      <property name="name" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="BED_NAME" sql-type="varchar(20)" length="20"/>
      </property>
      <property name="icon" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="BED_ICON" sql-type="varchar(50)" length="50"/>
      </property>
      <set name="rooms" table="ROOMBED" outer-join="false" inverse="true" lazy="true" optimistic-lock="true">
      	<key>
      		<column name="BED_ID" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<many-to-many class="com.tim11.pma.ftn.pmaprojekat.model.Room" outer-join="false">
      		<column name="ROOM_ID" sql-type="int" not-null="true" length="0"/>
      	</many-to-many>
      </set>
   </class>
</hibernate-mapping>