<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tim11.pma.ftn.pmaprojekat.model" auto-import="true">
   <class name="Price" table="PRICE" mutable="true" dynamic-update="false" dynamic-insert="false" select-before-update="false" lazy="true" abstract="false">
      <id name="priceId">
         <column name="PRICE_ID" sql-type="int" not-null="true"/>
      </id>
      <property name="startDate" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="PRICE_START_DATE" sql-type="date"/>
      </property>
      <property name="endDate" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="PRICE_END_DATE" sql-type="date"/>
      </property>
      <property name="value" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="PRICE_VALUE" sql-type="decimal(5,2)" length="5" precision="2"/>
      </property>
      <bag name="room" outer-join="false" lazy="true" optimistic-lock="true">
      	<key>
      		<column name="PRICE_ID" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<one-to-many class="com.tim11.pma.ftn.pmaprojekat.model.Room"/>
      </bag>
   </class>
</hibernate-mapping>