<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tim11.pma.ftn.pmaprojekat.model" auto-import="true">
   <class name="Reservation" table="RESERVATION" mutable="true" dynamic-update="false" dynamic-insert="false" select-before-update="false" lazy="true" abstract="false">
      <id name="reservationId">
         <column name="RESERVATION_ID" sql-type="int" not-null="true"/>
      </id>
      <property name="startDate" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="RESERVATION_START_DATE" sql-type="datetime"/>
      </property>
      <property name="endDate" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="RESERVATION_END_DATE" sql-type="datetime"/>
      </property>
      <property name="price" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="RESERVATION_PRICE" sql-type="decimal(7,2)" length="7" precision="2"/>
      </property>
      <property name="firstname" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="RESERVATION_FIRSTNAME" sql-type="varchar(30)" length="30"/>
      </property>
      <property name="lastname" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="RESERVATION_LASTNAME" sql-type="varchar(30)" length="30"/>
      </property>
      <property name="email" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="RESERVATION_EMAIL" sql-type="varchar(50)" length="50"/>
      </property>
      <many-to-one name="room" class="com.tim11.pma.ftn.pmaprojekat.model.Room" outer-join="false" update="true" insert="true">
      	<column name="ROOM_ID" sql-type="int" not-null="true" length="0"/>
      </many-to-one>
   </class>
</hibernate-mapping>