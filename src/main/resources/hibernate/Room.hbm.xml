<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tim11.pma.ftn.pmaprojekat.model" auto-import="true">
   <class name="Room" table="ROOM" mutable="true" dynamic-update="false" dynamic-insert="false" select-before-update="false" lazy="true" abstract="false">
      <id name="roomId">
         <column name="ROOM_ID" sql-type="int" not-null="true"/>
      </id>
      <property name="name" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="ROOM_NAME" sql-type="varchar(100)" length="100"/>
      </property>
      <property name="count" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="ROOM_COUNT" sql-type="int"/>
      </property>
      <property name="available" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="ROOM_AVAILABLE" sql-type="int"/>
      </property>
      <property name="description" insert="true" update="true" lazy="false" optimistic-lock="true">
         <column name="ROOM_DESCRIPTION" sql-type="varchar(200)" length="200"/>
      </property>
      <many-to-one name="hotel" class="com.tim11.pma.ftn.pmaprojekat.model.Hotel" outer-join="false" update="true" insert="true">
      	<column name="HOTEL_ID" sql-type="int" not-null="true" length="0"/>
      </many-to-one>
      <many-to-one name="price" class="com.tim11.pma.ftn.pmaprojekat.model.Price" outer-join="false" update="true" insert="true">
      	<column name="PRICE_ID" sql-type="int" not-null="true" length="0"/>
      </many-to-one>
      <set name="reservation" outer-join="false" lazy="true" optimistic-lock="true">
      	<key on-delete="noaction" unique="false">
      		<column name="ROOM_ID" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<one-to-many class="com.tim11.pma.ftn.pmaprojekat.model.Reservation"/>
      </set>
      <set name="beds" table="ROOMBED" outer-join="false" lazy="true" optimistic-lock="true">
      	<key on-delete="noaction" unique="false">
      		<column name="ROOM_ID" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<many-to-many class="com.tim11.pma.ftn.pmaprojekat.model.Bed" outer-join="false">
      		<column name="BED_ID" sql-type="int" not-null="true" length="0"/>
      	</many-to-many>
      </set>
      <set name="RoomAmenity" table="ROOMAMENITY" outer-join="false" lazy="true" optimistic-lock="true">
      	<key on-delete="noaction" unique="false">
      		<column name="ROOM_ID" sql-type="int" not-null="true" length="0"/>
      	</key>
      	<many-to-many class="com.tim11.pma.ftn.pmaprojekat.model.Amenity" outer-join="false">
      		<column name="AMENITY_ID" sql-type="int" not-null="true" length="0"/>
      	</many-to-many>
      </set>
   </class>
</hibernate-mapping>